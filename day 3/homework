from turtle import*
speed(7)
pensize(7)

# we want paint a house

penup()
goto(-350,100)
pendown()
color("black")

forward(150)
left(90)
forward(150)
left(90)
forward(150)
left(90)
forward(150)
left(90)
forward(70)
left(90)


color("pink")
begin_fill()
